{
  "name": "cdp-cli",
  "version": "0.0.2",
  "description": "CDP Command Line Interface",
  "main": "dist/cdp-cli.min.js",
  "scripts": {
    "build": "run-p build:*",
    "build:ts": "tsc && webpack",
    "prebuild:ts": "node ./tasks/build-ts-clean.js",
    "postbuild:ts": "node ./tasks/build-ts-normalize.js",
    "minify": "uglifyjs dist/cdp-cli.js -o dist/cdp-cli.min.js --comments /^!/ --source-map includeSources,url=cdp-cli.min.js.map",
    "test": "run-s lint unit-test",
    "pretest": "tsc -p ./tests/jasmine/tsconfig.json",
    "posttest": "cd ./docs/reports/coverage/_org && remap-istanbul -i coverage.json -o ../ -t html",
    "clean": "node ./tasks/clean.js",
    "depends": "npm list --depth=0",
    "depends:prod": "npm list --depth=0 --only=prod",
    "depends:dev": "npm list --depth=0 --only=dev",
    "lint": "run-s -l lint:*",
    "lint:js": "eslint tasks/**/*.js built/**/*.js -c tests/eslint/eslintrc.json",
    "lint:ts": "tslint --type-check -p ./tsconfig.json -c ./tests/tslint/tslint.json 'src/**/*.ts'",
    "unit-test": "istanbul cover ./node_modules/jasmine-node/bin/jasmine-node tests/jasmine/ --dir docs/reports/coverage/_org",
    "document": "typedoc --out docs/typedoc/ src/",
    "metrics": "plato -r -d docs/reports/metrics -e ./tests/eslint/eslintrc.json built/**/*.js",
    "ci": "run-s build test",
    "deploy": "run-s -l clean build minify test metrics document",
    "debug": "node --inspect --debug-brk ./node_modules/jasmine-node/bin/jasmine-node tests/jasmine/",
    "debug:tool": "node-debug ./node_modules/jasmine-node/bin/jasmine-node tests/jasmine/",
    "update": "npm update --save-dev && npm update --save && npm run deploy",
    "submodule": "node ./tasks/submodule.js",
    "libmodule": "node ./tasks/libmodule.js"
  },
  "bin": {
    "cdp": "bin/cdp"
  },
  "files": [
    "bin",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CDP-Tokyo/cdp-cli.git"
  },
  "keywords": [
    "CDP",
    "cli",
    "cordova",
    "electron",
    "TypeScript"
  ],
  "author": "Shin Ogata",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/CDP-Tokyo/cdp-cli/issues"
  },
  "homepage": "https://github.com/CDP-Tokyo/cdp-cli#readme",
  "dependencies": {
    "cdp-lib": "git+ssh://git@github.com/CDP-Tokyo/cdp-lib.git",
    "commander": "^2.9.0",
    "inquirer": "^3.0.6"
  },
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/commander": "^2.9.0",
    "@types/del": "^2.2.32",
    "@types/fs-extra": "^3.0.1",
    "@types/glob": "^5.0.30",
    "@types/inquirer": "0.0.34",
    "@types/jasmine": "^2.5.47",
    "@types/jquery": "^2.0.45",
    "@types/lodash": "^4.14.64",
    "@types/node": "^7.0.18",
    "@types/underscore.string": "0.0.30",
    "@types/uuid": "^2.0.29",
    "@types/which": "^1.0.28",
    "convert-source-map": "^1.5.0",
    "del": "^2.2.2",
    "dts-bundle": "^0.7.2",
    "eslint": "^3.19.0",
    "jasmine-node": "^2.0.0",
    "npm-run-all": "^4.0.2",
    "plato": "^1.7.0",
    "remap-istanbul": "^0.9.5",
    "source-map": "^0.5.6",
    "source-map-loader": "^0.2.1",
    "tslint": "^5.2.0",
    "typedoc": "^0.7.0",
    "typescript": "^2.3.2",
    "typescript-formatter": "^5.1.3",
    "uglify-es": "^3.0.7",
    "webpack": "^2.5.1"
  }
}
